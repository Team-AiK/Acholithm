    "def arraySwitch():\n",
    "    string=input(\"입력 :\")\n",
    "    arr=string.split(\" \")\n",
    "    div=int(len(arr)/2)\n",
    "    for i in range(1,int(len(arr)/2)):\n",
    "            arr.insert(1+div,arr[1])\n",
    "            div+=1\n",
    "            del arr[1]\n",
    "    print(\"출력 :\",arr)\n",
    "        \n",
    "arraySwitch()"
   ]
